<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ca.dal.csci3130.coursesmanagementsystem">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login" />
        <activity android:name=".IntermediaryActivity" />
        <activity android:name=".academicTimeTable" />
        <activity android:name=".courseRecommend" />
        <activity android:name=".moreInfo" />
        <activity android:name=".TaInfo" />
        <activity android:name=".Year1" />
        <activity android:name=".Year4" />
        <activity android:name=".Year3" />
        <activity android:name=".Year2" />
        <activity android:name=".level" />
        <activity android:name=".CourseRegisterActivity" />
        <activity android:name=".UserActivity"></activity>
    </application>

</manifest>